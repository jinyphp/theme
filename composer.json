{
    "name": "jiny/theme",
    "description": "jinyPHP Theme Manager for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "hojinlee",
            "email": "infohojin@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.3|^8.0"

    },
    "autoload": {
        "psr-4": {
            "Jiny\\Theme\\":"src/"
        },
        "files" : [
            "src/Helpers/Helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Jiny\\Theme\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jiny\\Theme\\JinyThemeServiceProvider"
            ]
        }
    }

}
